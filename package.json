{
  "name": "gatsby",
  "version": "4.0.0",
  "private": true,
  "scripts": {
    "develop": "gatsby develop --host 0.0.0.0 --port 8000",
    "build": "gatsby build",
    "serve": "gatsby serve --host 0.0.0.0 --port 8000",
    "makejson": "python content/csvToJson.py && npx prettier --write content/json",
    "format": "prettier . --write",
    "lint": "eslint . --fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@fontsource/roboto": "^4.4.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-brands-svg-icons": "^5.15.3",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@material-ui/core": "latest",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/styles": "latest",
    "gatsby": "latest",
    "gatsby-plugin-manifest": "^3.6.0",
    "gatsby-plugin-material-ui": "latest",
    "gatsby-plugin-offline": "^4.6.0",
    "gatsby-plugin-react-helmet": "latest",
    "react": "latest",
    "react-dom": "latest",
    "react-helmet": "latest",
    "tiny-relative-date": "^1.3.0",
    "typescript": "^4.3.2"
  },
  "devDependencies": {
    "@types/react-helmet": "^6.1.1",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "eslint": "^7.27.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.8.1"
  }
